<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.german.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>

        </activity>
        <activity android:name="com.example.german.myapplication.Agregar"></activity>
        <activity android:name="com.example.german.myapplication.BuscarPersona"></activity>
        <activity android:name="com.example.german.myapplication.modificar"></activity>
        <activity android:name="com.example.german.myapplication.eliminar"></activity>
        <activity android:name="com.example.german.myapplication.pre_modificar"></activity>

    </application>

</manifest>